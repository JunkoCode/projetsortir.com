{% extends 'base.html.twig' %}

{% block title %}Etat{% endblock %}

{% block body %}
    <div class="container">
        <h1 class="my-5"><i class="ion-ios-paper-outline"></i> Etat "{{ etat.libelle }}" </h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ etat.id }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ etat.libelle }}</td>
            </tr>
        </tbody>
    </table>

    <a class="btn btn-outline-dark" href="{{ path('app_etat_index') }}">Retour</a>

    <a class="btn btn-outline-warning" href="{{ path('app_etat_edit', {'id': etat.id}) }}">Modifier</a>

    </div>
{% endblock %}
